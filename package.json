{
  "name": "ng2-jspm-template",
  "version": "0.0.1",
  "description": "A template for a quick development workflow with angular 2 and jspm",
  "author": {
    "name": "Sven Reglitzki",
    "email": "sreglitzki@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flaviait/ng2-jspm-template.git"
  },
  "jspm": {
    "name": "app",
    "main": "app.js",
    "directories": {
      "lib": "src"
    },
    "dependencies": {
      "@angular/forms": "npm:@angular/forms@^0.2.0",
      "@angular/platform-browser-dynamic": "npm:@angular/platform-browser-dynamic@^2.0.0-rc.4",
      "@ngrx/router-store": "npm:@ngrx/router-store@^0.0.1",
      "es6-shim": "github:es-shims/es6-shim@^0.35.0",
      "immutable": "npm:immutable@^3.8.1",
      "lodash": "npm:lodash@^4.13.1",
      "ng2-translate": "npm:ng2-translate@^2.2.2",
      "reflect-metadata": "npm:reflect-metadata@^0.1.3"
    },
    "devDependencies": {
      "json": "github:systemjs/plugin-json@^0.1.2",
      "net": "github:jspm/nodelibs-net@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.12",
      "plugin-typescript": "github:frankwallis/plugin-typescript@5.0.8",
      "scss": "github:KevCJones/plugin-scss@^0.2.11",
      "systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.5.8",
      "tty": "github:jspm/nodelibs-tty@^0.2.0-alpha"
    },
    "peerDependencies": {
      "@angular/common": "npm:@angular/common@^2.0.0-rc.4",
      "@angular/compiler": "npm:@angular/compiler@^2.0.0-rc.4",
      "@angular/core": "npm:@angular/core@^2.0.0-rc.4",
      "@angular/http": "npm:@angular/http@2.0.0-rc.4",
      "@angular/platform-browser": "npm:@angular/platform-browser@^2.0.0-rc.4",
      "@ngrx/core": "npm:@ngrx/core@^1.0.0",
      "@ngrx/router": "npm:@ngrx/router@^1.0.0-beta.2",
      "@ngrx/store": "npm:@ngrx/store@^2.0.1",
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "rxjs": "npm:rxjs@5.0.0-beta.6",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha",
      "zone.js": "npm:zone.js@^0.6.6"
    },
    "overrides": {
      "npm:@angular/common@2.0.0-rc.4": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:@angular/compiler@2.0.0-rc.4": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:@angular/core@2.0.0-rc.4": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:@angular/forms@0.2.0": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:@angular/http@2.0.0-rc.4": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:@angular/platform-browser-dynamic@2.0.0-rc.4": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:@angular/platform-browser@2.0.0-rc.4": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:@ngrx/core@1.0.1": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:@ngrx/router-store@0.0.1": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:@ngrx/router@1.0.0-beta.2": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:@ngrx/store@2.0.1": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:immutable@3.8.1": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:lodash@4.13.1": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:ng2-translate@2.2.2": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:rxjs@5.0.0-beta.6": {
        "meta": {
          "*.js": {
            "typings": true
          }
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:reflect-metadata@0.1.3": {
        "map": {
          "crypto": "@empty"
        }
      },
      "npm:sass.js@0.9.11": {
        "map": {
          "ws": "@empty"
        },
        "meta": {
          "dist/sass.worker.js": {
            "format": "global"
          }
        }
      },
      "npm:typescript@2.0.0": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  },
  "scripts": {
    "check": "tslint \"src/**/*.ts\" -e \"src/generated/**/*.ts\"",
    "dev": "concurrently -r \"npm run dev-bundle\" \"npm run dev-start\" \"npm run dev-test\" \"npm run dev-styles\" \"npm run dev-generate-typings\" \"npm run dev-translations\"",
    "dev-build": "npm run dev-bundle",
    "dev-bundle": "npm run generate-typings && npm run translations && jspm bundle -diw \"src/**/*.ts\" .tmp/dev-bundle.js --config jspm.dev-bundle.json --log ok",
    "dev-generate-typings": "node bin/write-typings.js --watch \"src/app/**/*.scss\" \"src/generated/typings.d.ts\"",
    "dev-install": "npm i && jspm i && typings i",
    "dev-start": "node server/server-dev.js",
    "dev-styles": "mkdirp .tmp && node bin/process-styles.js --watch \"src/styles/**/*.scss\" src/styles/main.scss .tmp/main.css",
    "dev-test": "wait-for-change .tmp/dev-bundle.js && karma start karma-dev.conf.js",
    "dev-translations": "mkdirp .tmp && node bin/process-translations.js --watch \"src/**/*.i18n.yml\" src/generated/translations.ts",
    "dist": "concurrently -r \"npm run dist-bundle\" \"npm run dist-test\" \"npm run dist-styles\"",
    "dist-bundle": "rimraf dist/bundle.js && npm run generate-typings && npm run translations && jspm build src/main dist/bundle.js --minify --format global --global-name APP --config jspm.dist-bundle.json --log ok && cp src/index.html dist",
    "dist-server": "npm run dist && npm run dist-start",
    "dist-start": "node server/server-dist.js",
    "dist-styles": "rimraf \"dist/*.css\" \"dist/*.css.map\" && mkdirp dist && node bin/process-styles.js --minify src/styles/main.scss dist/main.css",
    "dist-test": "rimraf .tmp/test-bundle.js && npm run generate-typings && npm run translations && npm run dist-test-bundle && karma start karma-dist.conf.js",
    "dist-test-bundle": "jspm bundle -d \"src/**/*.ts\" .tmp/test-bundle.js --config jspm.dev-bundle.json --log ok",
    "translations": "mkdirp .tmp && node bin/process-translations.js \"src/**/*.i18n.yml\" src/generated/translations.ts",
    "generate-typings": "node bin/write-typings.js \"src/app/**/*.scss\" \"src/generated/typings.d.ts\"",
    "start": "npm run dev",
    "test": "jspm i && typings i && npm run check && npm run dist-test"
  },
  "devDependencies": {
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/core": "^2.0.0-rc.4",
    "@angular/forms": "^0.2.0",
    "@angular/http": "^2.0.0-rc.4",
    "@angular/platform-browser": "^2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.4",
    "@ngrx/core": "^1.0.1",
    "@ngrx/router": "^1.0.0-beta.2",
    "@ngrx/router-store": "0.0.1",
    "@ngrx/store": "^2.0.1",
    "autoprefixer": "^6.3.7",
    "chokidar": "^1.6.0",
    "chokidar-socket-emitter": "^0.5.2",
    "commander": "^2.9.0",
    "concurrently": "^2.2.0",
    "cssnano": "^3.7.3",
    "exorcist": "^0.4.0",
    "glob": "^7.0.5",
    "http-proxy": "^1.13.2",
    "immutable": "^3.8.1",
    "jasmine-core": "2.4.1",
    "jasmine-reporters": "2.2.0",
    "js-yaml": "^3.6.1",
    "jspm": "^0.17.0-beta.22",
    "karma": "^1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-jspm": "github:Workiva/karma-jspm#master",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "livereload": "^0.5.0",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "ng2-translate": "^2.2.2",
    "node-sass": "^3.8.0",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss": "^5.1.0",
    "postcss-cli": "^2.5.2",
    "rimraf": "^2.5.3",
    "rxjs": "^5.0.0-beta.6",
    "systemjs-hot-reloader": "^0.5.8",
    "tslint": "^3.13.0",
    "typescript": "^2.0.0",
    "typings": "^1.3.2",
    "wait-for-change": "^1.0.2",
    "zone.js": "^0.6.12"
  },
  "dependencies": {
    "express": "^4.14.0",
    "http-proxy": "^1.14.0"
  },
  "engines": {
    "node": ">= 4.0.0"
  }
}
