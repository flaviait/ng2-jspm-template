{
  "name": "ng2-jspm-template",
  "version": "0.3.0",
  "description": "A template for a quick development workflow with angular 2 and jspm",
  "author": {
    "name": "Sven Reglitzki",
    "email": "sreglitzki@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flaviait/ng2-jspm-template.git"
  },
  "scripts": {
    "dist": "dist && jspm build src/main dist/bundle.js --minify --format global --global-name APP --config jspm.dist-bundle.json --log ok",
    "start": "[ \"$NODE_ENV\" == production ] && node server/server-dist.js || concurrently -r \"dev\" \"wait-for-change src/generated/translations.ts && jspm bundle -diw 'src/**/*.ts' .tmp/dev-bundle.js --config jspm.dev-bundle.json --log ok\"",
    "test": "compile-translations && jspm bundle -d \"src/**/*.spec.ts\" .tmp/test-bundle.js --config jspm.dev-bundle.json --log ok && tests",
    "postinstall": "[ \"$NODE_ENV\" == production ] && exit 0 || touch jspm.browser.js && jspm i"
  },
  "jspm": {
    "name": "app",
    "main": "app.js",
    "directories": {
      "lib": "src"
    },
    "dependencies": {
      "@angular/forms": "npm:@angular/forms@^2.2.1",
      "@angular/router": "npm:@angular/router@^3.2.1",
      "@types/jasmine": "npm:@types/jasmine@^2.5.38",
      "@types/lodash": "npm:@types/lodash@^4.14.39",
      "es6-shim": "github:es-shims/es6-shim@^0.35.0",
      "immutable": "npm:immutable@^3.8.1",
      "lodash": "npm:lodash@^4.17.2",
      "ng2-translate": "npm:ng2-translate@^2.5.0",
      "reflect-metadata": "npm:reflect-metadata@^0.1.3"
    },
    "devDependencies": {
      "http": "npm:jspm-nodelibs-http@^0.2.0",
      "json": "github:systemjs/plugin-json@^0.1.2",
      "net": "npm:jspm-nodelibs-net@^0.2.0",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.12",
      "plugin-typescript": "github:frankwallis/plugin-typescript@5.3.1",
      "scss": "github:KevCJones/plugin-scss@^0.2.11",
      "systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.6.0",
      "tty": "npm:jspm-nodelibs-tty@^0.2.0",
      "url": "npm:jspm-nodelibs-url@^0.2.0"
    },
    "peerDependencies": {
      "@angular/common": "npm:@angular/common@2.2.1",
      "@angular/compiler": "npm:@angular/compiler@^2.2.1",
      "@angular/core": "npm:@angular/core@2.2.1",
      "@angular/http": "npm:@angular/http@2.2.1",
      "@angular/platform-browser": "npm:@angular/platform-browser@2.2.1",
      "@angular/platform-browser-dynamic": "npm:@angular/platform-browser-dynamic@^2.2.1",
      "@ngrx/core": "npm:@ngrx/core@^1.2.0",
      "@ngrx/store": "npm:@ngrx/store@^2.2.1",
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "rxjs": "npm:rxjs@5.0.0-beta.12",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "timers": "npm:jspm-nodelibs-timers@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0",
      "zone.js": "npm:zone.js@^0.6.21"
    },
    "overrides": {
      "github:socketio/socket.io-client@1.5.0": {
        "main": "socket.io.js"
      },
      "npm:@angular/common@2.2.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/compiler@2.2.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/core@2.2.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/http@2.2.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/platform-browser-dynamic@2.2.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/platform-browser@2.2.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/router@3.2.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/router": ".",
          "./testing": "./bundles/router-testing.umd.js"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.17.2": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:reflect-metadata@0.1.8": {
        "map": {
          "crypto": "@empty"
        }
      },
      "npm:sass.js@0.9.13": {
        "map": {
          "ws": "@empty"
        },
        "meta": {
          "dist/sass.worker.js": {
            "format": "global"
          }
        }
      },
      "npm:typescript@2.0.10": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  },
  "devDependencies": {
    "@angular/common": "^2.2.1",
    "@angular/compiler": "^2.2.1",
    "@angular/core": "^2.2.1",
    "@angular/forms": "^2.2.1",
    "@angular/http": "^2.2.1",
    "@angular/platform-browser": "^2.2.1",
    "@angular/platform-browser-dynamic": "^2.2.1",
    "@angular/router": "^3.2.1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "concurrently": "^3.1.0",
    "http-proxy": "^1.15.2",
    "immutable": "^3.8.1",
    "jspm": "^0.17.0-beta.31",
    "lodash": "^4.17.2",
    "moment": "^2.16.0",
    "ng2-jspm-template-libs": "~0.2",
    "ng2-translate": "^2.5.0",
    "postcss-cli": "^2.6.0",
    "rimraf": "^2.5.4",
    "rxjs": "5.0.0-beta.12",
    "typescript": "^2.0.10",
    "wait-for-change": "^1.1.0",
    "zone.js": "0.6.26"
  },
  "dependencies": {
    "express": "^4.14.0",
    "http-proxy": "^1.14.0"
  },
  "engines": {
    "node": ">=6.9",
    "npm": ">=3.10"
  }
}
