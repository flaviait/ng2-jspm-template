{
  "name": "ng2-jspm-template",
  "version": "0.0.1",
  "description": "A template for a quick development workflow with angular 2 and jspm",
  "author": {
    "name": "Sven Reglitzki",
    "email": "sreglitzki@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flaviait/ng2-jspm-template.git"
  },
  "jspm": {
    "name": "app",
    "main": "app.js",
    "directories": {
      "lib": "src"
    },
    "dependencies": {
      "@angular/forms": "npm:@angular/forms@^2.0.0",
      "@angular/router": "npm:@angular/router@^3.0.0",
      "@types/jasmine": "npm:@types/jasmine@^2.2.33",
      "@types/lodash": "npm:@types/lodash@^4.14.34",
      "es6-shim": "github:es-shims/es6-shim@^0.35.0",
      "immutable": "npm:immutable@^3.8.1",
      "lodash": "npm:lodash@^4.13.1",
      "ng2-translate": "npm:ng2-translate@^2.5.0",
      "reflect-metadata": "npm:reflect-metadata@^0.1.3"
    },
    "devDependencies": {
      "http": "npm:jspm-nodelibs-http@^0.2.0",
      "json": "github:systemjs/plugin-json@^0.1.2",
      "net": "npm:jspm-nodelibs-net@^0.2.0",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.12",
      "plugin-typescript": "github:frankwallis/plugin-typescript@5.2.9",
      "scss": "github:KevCJones/plugin-scss@^0.2.11",
      "systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.6.0",
      "tty": "npm:jspm-nodelibs-tty@^0.2.0",
      "url": "npm:jspm-nodelibs-url@^0.2.0"
    },
    "peerDependencies": {
      "@angular/common": "npm:@angular/common@2.1.1",
      "@angular/compiler": "npm:@angular/compiler@^2.0.0",
      "@angular/core": "npm:@angular/core@2.1.1",
      "@angular/http": "npm:@angular/http@2.0.0",
      "@angular/platform-browser": "npm:@angular/platform-browser@2.1.1",
      "@angular/platform-browser-dynamic": "npm:@angular/platform-browser-dynamic@^2.0.0",
      "@ngrx/core": "npm:@ngrx/core@^1.2.0",
      "@ngrx/store": "npm:@ngrx/store@^2.1.2",
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "rxjs": "npm:rxjs@5.0.0-beta.12",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "timers": "npm:jspm-nodelibs-timers@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0",
      "zone.js": "npm:zone.js@^0.6.21"
    },
    "overrides": {
      "github:socketio/socket.io-client@1.5.0": {
        "main": "socket.io.js"
      },
      "npm:@angular/common@2.1.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/compiler@2.1.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/core@2.1.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/http@2.1.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/platform-browser-dynamic@2.1.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/platform-browser@2.1.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/router@3.1.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/router": ".",
          "./testing": "./bundles/router-testing.umd.js"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.16.4": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:reflect-metadata@0.1.8": {
        "map": {
          "crypto": "@empty"
        }
      },
      "npm:sass.js@0.9.13": {
        "map": {
          "ws": "@empty"
        },
        "meta": {
          "dist/sass.worker.js": {
            "format": "global"
          }
        }
      },
      "npm:typescript@2.0.6": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  },
  "scripts": {
    "dev": "concurrently -r \"npm run dev-test\" \"npm run dev-bundle\" \"npm run watch\" \"npm run dev-start\"",
    "dev-bundle": "wait-for-change src/generated/translations.ts .tmp/main.css && npm run touch-jspm-browser && jspm bundle -diw \"src/**/*.ts\" .tmp/dev-bundle.js --config jspm.dev-bundle.json --log ok",
    "dev-install": "npm i && jspm i",
    "dev-start": "node server/server-dev.js",
    "dev-test": "wait-for-change .tmp/dev-bundle.js && karma start karma-dev.conf.js",
    "dist": "concurrently -r \"npm run dist-bundle\" \"npm run dist-test\" \"npm run dist-styles\"",
    "dist-bundle": "rimraf dist/bundle.js && npm run translations && jspm build src/main dist/bundle.js --minify --format global --global-name APP --config jspm.dist-bundle.json --log ok && cp src/index.html dist",
    "dist-server": "npm run dist && npm run dist-start",
    "dist-start": "node server/server-dist.js",
    "dist-styles": "rimraf \"dist/*.css\" \"dist/*.css.map\" && mkdirp dist && node bin/compile-styles.js --minify src/styles/main.scss dist/main.css",
    "dist-test": "rimraf .tmp/test-bundle.js && npm run translations && npm run dist-test-bundle && karma start karma-dist.conf.js",
    "dist-test-bundle": "jspm bundle -d \"src/**/*.ts\" .tmp/test-bundle.js --config jspm.dev-bundle.json --log ok",
    "lint-scripts": "node bin/lint-scripts.js \"src/**/*.ts\" -e \"src/generated/**/*.ts\"",
    "lint-styles": "node bin/lint-styles.js \"src/**/*.scss\"",
    "touch-jspm-browser": "node bin/touch.js jspm.browser.js",
    "translations": "node bin/compile-translations.js \"src/**/*.i18n.yml\" src/generated/translations.ts",
    "start": "npm run dev",
    "test": "jspm i && npm run lint-scripts && npm run lint-styles && npm run dist-test",
    "watch-lint-scripts": "node bin/lint-scripts.js -w \"src/**/*.ts\" -e \"src/generated/**/*.ts\"",
    "watch-lint-styles": "node bin/lint-styles.js -w \"src/**/*.scss\"",
    "watch-styles": "node bin/compile-styles.js -w \"src/styles/**/*.scss\" src/styles/main.scss .tmp/main.css",
    "watch-translations": "node bin/compile-translations.js -w \"src/**/*.i18n.yml\" src/generated/translations.ts",
    "watch": "concurrently -r \"npm run watch-translations\" \"npm run watch-styles\" \"npm run watch-lint-scripts\" \"npm run watch-lint-styles\""
  },
  "devDependencies": {
    "@angular/common": "^2.1.1",
    "@angular/compiler": "^2.1.1",
    "@angular/core": "^2.1.1",
    "@angular/forms": "^2.1.1",
    "@angular/http": "^2.1.1",
    "@angular/platform-browser": "^2.1.1",
    "@angular/platform-browser-dynamic": "^2.1.1",
    "@angular/router": "^3.1.1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "autoprefixer": "^6.5.1",
    "chokidar": "^1.6.1",
    "chokidar-socket-emitter": "git://github.com/svi3c/chokidar-socket-emitter.git#add-quiet-option",
    "commander": "^2.9.0",
    "concurrently": "^3.1.0",
    "cssnano": "^3.8.0",
    "exorcist": "^0.4.0",
    "glob": "^7.1.1",
    "http-proxy": "^1.15.2",
    "immutable": "^3.8.1",
    "jasmine-core": "2.5.2",
    "jasmine-reporters": "2.2.0",
    "js-yaml": "^3.6.1",
    "jspm": "^0.17.0-beta.31",
    "karma": "^1.3.0",
    "karma-jasmine": "1.0.2",
    "karma-jspm": "^2.2.0",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "livereload": "^0.6.0",
    "lodash": "^4.16.4",
    "log4js": "^0.6.38",
    "mkdirp": "^0.5.1",
    "moment": "^2.15.2",
    "ng2-translate": "^3.1.3",
    "node-sass": "^3.10.1",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss": "^5.2.5",
    "postcss-cli": "^2.6.0",
    "rimraf": "^2.5.4",
    "rxjs": "5.0.0-rc.1",
    "stylelint": "^7.5.0",
    "systemjs-hot-reloader": "^0.6.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.6",
    "wait-for-change": "^1.1.0",
    "zone.js": "0.6.26"
  },
  "dependencies": {
    "express": "^4.14.0",
    "http-proxy": "^1.14.0"
  },
  "engines": {
    "node": ">=6.9",
    "npm": ">=3.10"
  }
}
